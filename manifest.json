{
  "name": "Personal Web Informatics",
  "version": "0.1",
  "description": "A tool that looks at your web history and tells you about it",

  "permissions": [
    "tabs",
    "bookmarks",
    "history",
    "management", 
    "unlimitedStorage",
    "notifications",
    "background"
  ],
  "app": {
    "launch":{
        "local_path":"index.html"
    }
  },
  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  }, 
  "background_page":"background.html"
}
